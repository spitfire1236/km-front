{
    "name": "@kontent-market/eslint-config",
    "version": "1.0.0",
    "description": "kontent market ESlint config",
    "author": "Alexey Chernyshev <er1x4ever@gmail.com>",
    "homepage": "https://github.com/spitfire77/km-front#readme",
    "license": "MIT",
    "main": "index.js",
    "files": [
        "index.js",
        "react.js",
        "README.md",
        "rules"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/spitfire77/km-front.git",
        "directory": "packages/eslint-config"
    },
    "scripts": {
        "publish": "clean-publish",
        "release": "yarn standard-version -t @kontent-market/eslint-config@"
