    },
    "devDependencies": {
        "clean-publish": "^2.0.0",
        "commitizen": "^4.2.2",
        "cz-conventional-changelog": "^3.3.0",
        "standard-version": "^9.0.0"
    },
    "peerDependencies": {
        "babel-eslint": "^10",
        "eslint": "^7.2",
        "eslint-config-prettier": "^8",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23",
        "eslint-plugin-react-hooks": "^4",
        "eslint-plugin-unicorn": "^29"
    }
